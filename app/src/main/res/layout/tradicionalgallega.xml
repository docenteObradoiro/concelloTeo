<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9FBF9"
    tools:context=".redDeRutas.Ruta6">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <ImageButton
            android:id="@+id/logoTeo"
            android:layout_width="100dp"
            android:layout_height="60dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="logoConcello"
            android:foreground="?android:attr/selectableItemBackground"
            android:scaleType="fitXY"
            android:theme="@android:style/ThemeOverlay.Material.Dark"
            app:srcCompat="@drawable/logo" />

        <ImageButton
            android:id="@+id/menu"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginLeft="205dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="menu hamburguesa"
            android:foreground="?android:attr/selectableItemBackground"
            android:scaleType="fitXY"
            android:theme="@android:style/ThemeOverlay.Material.Dark"
            app:srcCompat="@drawable/hamburguesa"
            tools:ignore="TouchTargetSizeCheck" />
    </RelativeLayout>


    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/titulo"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:text="@string/ondecomer"
                android:textColor="#415419"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="40dp"
                android:foregroundTint="#2DFFFFFF"
                android:translationY="-50dp"
                app:srcCompat="@mipmap/lineaverder" />

            <RelativeLayout
                android:background="@drawable/shape"
                android:layout_width="300dp"
                android:layout_height="280dp"
                android:orientation="horizontal"
                android:layout_marginTop="-10dp"
                android:layout_marginLeft="35dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp">

                <ImageButton
                    android:id="@+id/logoLocalParadaDeFrancos"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoLocalParadaDeFrancos"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/restaurante" />

                <ImageButton
                    android:id="@+id/logoDireccionParadaDeFrancos"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/logoLocalParadaDeFrancos"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoDireccionParadaDeFrancos"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/mapsnegro" />

                <ImageButton
                    android:id="@+id/logoTelefonoParadaDeFrancos"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/logoDireccionParadaDeFrancos"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoTelefonoParadaDeFrancos"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/telefonoNegro" />

                <ImageButton
                    android:id="@+id/logoInfoParadaDeFrancos"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/logoTelefonoParadaDeFrancos"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoInfo"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/globo" />

                <TextView
                    android:id="@+id/localParadaDeFrancos"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="5dp"
                    android:layout_toEndOf="@+id/logoLocalParadaDeFrancos"
                    android:gravity="center_vertical"
                    android:text="@string/paradaDeFrancos"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/direccionParadaDeFrancos"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/localParadaDeFrancos"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_toEndOf="@+id/logoDireccionParadaDeFrancos"
                    android:gravity="center_vertical"
                    android:text="@string/direccionParadaDeFrancos"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/telefonoParadaDeFrancos"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/direccionParadaDeFrancos"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_toEndOf="@+id/logoTelefonoParadaDeFrancos"
                    android:gravity="center_vertical"
                    android:text="@string/telefonoParaDeFrancos"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/emailParadaDeFrancos"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_below="@+id/telefonoParadaDeFrancos"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_toEndOf="@+id/logoInfoParadaDeFrancos"
                    android:gravity="center_vertical"
                    android:text="@string/emailParaDeFrancos"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/webParadaDeFrancos"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_below="@+id/emailParadaDeFrancos"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="25dp"
                    android:layout_toEndOf="@+id/logoInfoParadaDeFrancos"
                    android:gravity="center_vertical"
                    android:text="@string/webParadaDeFrancos"
                    android:textColor="#415419"
                    android:textSize="12sp" />
            </RelativeLayout>



            <RelativeLayout
                android:background="@drawable/shape"
                android:layout_width="300dp"
                android:layout_height="280dp"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="35dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp">

                <ImageButton
                    android:id="@+id/logoLocalOSonDoMinho"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoLocalOSonDoMinho"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/restaurante" />

                <ImageButton
                    android:id="@+id/logoDireccionOSonDoMinho"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/logoLocalOSonDoMinho"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoDireccionOSonDoMinho"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/mapsnegro" />

                <ImageButton
                    android:id="@+id/logoTelefonoOSonDoMinho"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/logoDireccionOSonDoMinho"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoTelefonoOSonDoMinho"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/telefonoNegro" />

                <ImageButton
                    android:id="@+id/logoInfoOSonDoMinho"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_below="@+id/logoTelefonoOSonDoMinho"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:background="#00FFFFFF"
                    android:backgroundTint="#00FFFFFF"
                    android:contentDescription="logoInfoOSonDoMinho"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/globo" />

                <TextView
                    android:id="@+id/localOSonDoMinho"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="5dp"
                    android:layout_toEndOf="@+id/logoLocalOSonDoMinho"
                    android:gravity="center_vertical"
                    android:text="@string/oSonDoMiño"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/direccionOSonDoMinho"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/localOSonDoMinho"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_toEndOf="@+id/logoDireccionOSonDoMinho"
                    android:gravity="center_vertical"
                    android:text="@string/direccionOSonDoMiño"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/telefonoOSonDoMinho"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/direccionOSonDoMinho"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_toEndOf="@+id/logoTelefonoOSonDoMinho"
                    android:gravity="center_vertical"
                    android:text="@string/telefonoOSonDoMiño"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/emailOSonDoMinho"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_below="@+id/telefonoOSonDoMinho"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_toEndOf="@+id/logoInfoOSonDoMinho"
                    android:gravity="center_vertical"
                    android:text="@string/emailOSonDoMiño"
                    android:textColor="#415419"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/webOSonDoMinho"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_below="@+id/emailOSonDoMinho"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="25dp"
                    android:layout_toEndOf="@+id/logoInfoOSonDoMinho"
                    android:gravity="center_vertical"
                    android:text="@string/webOSonDoMiño"
                    android:textColor="#415419"
                    android:textSize="12sp" />

            </RelativeLayout>


            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="362dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="40dp"
                android:foregroundTint="#2DFFFFFF"
                app:srcCompat="@mipmap/lineaverder" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">


                <ImageButton
                    android:id="@+id/facebook"
                    android:layout_width="53dp"
                    android:layout_height="48dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="#00B8AEAE"
                    android:contentDescription="facebook"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/facebook"
                    app:tint="#7F000000" />

                <ImageButton
                    android:id="@+id/twitter"
                    android:layout_width="70dp"
                    android:layout_height="48dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="-25dp"
                    android:layout_marginBottom="10dp"
                    android:layout_toEndOf="@+id/facebook"
                    android:backgroundTint="#00B8AEAE"
                    android:contentDescription="twitter"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/twitter"
                    app:tint="#7F000000" />

                <ImageButton
                    android:id="@+id/instagram"
                    android:layout_width="55dp"
                    android:layout_height="50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="10dp"
                    android:layout_marginStart="-25dp"
                    android:layout_toEndOf="@+id/twitter"
                    android:backgroundTint="#00B8AEAE"
                    android:contentDescription="instagram"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/instagram"
                    app:tint="#7F000000" />

                <ImageButton
                    android:id="@+id/wikiloc1"
                    android:layout_width="55dp"
                    android:layout_height="50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="10dp"
                    android:layout_marginStart="-15dp"
                    android:layout_toEndOf="@+id/instagram"
                    android:backgroundTint="#00B8AEAE"
                    android:contentDescription="wikiloc"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/wikiloc"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:tint="#7F000000" />

                <ImageButton
                    android:id="@+id/info"
                    android:layout_width="55dp"
                    android:layout_height="50dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="4dp"
                    android:backgroundTint="#00B8AEAE"
                    android:contentDescription="información"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:scaleType="fitXY"
                    android:theme="@android:style/ThemeOverlay.Material.Dark"
                    app:srcCompat="@drawable/info"
                    app:tint="#7F000000" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>